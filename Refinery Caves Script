local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Refinaray Caves Script", "Synapse")

local Tab = Window:NewTab("Teleport")
--Teleports--
local Section = Tab:NewSection("Made by | count egg#0111")

Section:NewButton("Stone shop", "Teleport to Stone shop inside of mines", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(745, 5, 60)
end)
Section:NewButton("Cave entrance", "Teleport to Cave entrance outside of mines", function()
     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(60, 5, 15)
end)
Section:NewButton("MainShop", "Teleport to MainShop", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1025, 6, -610)
end)
Section:NewButton("Mines(Outside of MainShop)", "Teleport to mines", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1155, 6, -600)
end)
Section:NewButton("SellArea", "Teleport to SellArea", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-465, 7, -65)
end)
Section:NewButton("Secret Room", "Teleport to secret room and brag to your friends", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(132, 90, 1080)
end)
Section:NewButton("TrainEnd", "Teleport to the end of train tracks and dont waste 300 bucks", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-160, 200, 500)
end)
Section:NewButton("Maze", "Teleport to the Maze", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1121, -198, 540)
end)
--end of teleports--
local Tab = Window:NewTab("Credits")
local Section = Tab:NewSection("Credits")
Section:NewLabel("Credits to count egg#0111")
Section:NewLabel("Credits to the person who gave me this idea")

local Tab = Window:NewTab("Updates Coming Soon")
local Section = Tab:NewSection("More Updates")
